{
  "name": "kin-home-sales-pipeline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:report": "vitest run --coverage && open tests/coverage/index.html",
    "test:integration": "playwright test tests/integration",
    "test:integration:ui": "playwright test tests/integration --ui",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:offline": "playwright test tests/integration/offline.spec.ts",
    "test:ci": "npm run setup:all:with-data && vitest run --coverage && playwright test",
    "test:smoke": "playwright test tests/integration/production-smoke.spec.ts",
    "premerge": "vitest run && playwright test --grep-invert @prod",
    "playwright:install": "playwright install",
    "setup:env": "node scripts/setup-env.js",
    "setup:db": "node scripts/setup-database.js",
    "setup:seed": "node scripts/seed-users.js",
    "setup:test-data": "node scripts/seed-test-projects.js",
    "setup:health": "node scripts/health-check.js",
    "setup:all": "npm run setup:env && npm run setup:db && npm run setup:seed && npm run setup:health",
    "setup:all:with-data": "npm run setup:all && npm run setup:test-data",
    "migrate:settings": "node scripts/run-settings-migration.js",
    "audit:prod": "npm audit --production",
    "audit:fix": "npm audit fix --production",
    "audit:report": "npm audit --production --json > docs/audit-report.json",
    "build:analyze": "ANALYZE=true npm run build",
    "deploy:preview": "vercel",
    "test:critical": "vitest run && playwright test --grep @critical",
    "predeploy": "npm run type-check && npm run lint && npm run test:critical && npm run audit:prod",
    "deploy:production": "npm run predeploy && vercel --prod",
    "migrate:offices": "node scripts/run-offices-migration.js",
    "migrate:system": "node scripts/run-system-migration.js",
    "migrate:audit": "node scripts/run-audit-migration.js"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@tanstack/react-query": "^5.51.23",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vercel/postgres": "^0.9.0",
    "autoprefixer": "^10.4.19",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.33",
    "idb": "^8.0.0",
    "lucide-react": "^0.408.0",
    "next": "^14.2.33",
    "next-auth": "^4.24.7",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "use-debounce": "^10.0.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.3.1",
    "@types/serviceworker": "^0.0.95",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "dotenv": "^16.4.5",
    "jsdom": "^24.1.0",
    "vitest": "^1.6.0"
  }
}
